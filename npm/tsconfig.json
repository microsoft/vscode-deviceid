{
	"compilerOptions": {
		"outDir": "./dist",
		"noEmitOnError": false,
		"strict": true,
		"target": "ES2020",
		"module": "CommonJS",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		"inlineSources": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noUncheckedIndexedAccess": true,
		"moduleResolution": "node"
	}
}
